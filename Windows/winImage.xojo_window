#tag WindowCode
	#tag Event
		Sub Resized()
		  writeImage()
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub init(p as Picture, sTitle as string)
		  myPicture = p
		  me.title = sTitle
		  
		  writeImage()
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub writeImage()
		  if myPicture <> nil then
		    pictureToCanvas(myPicture, Canvas1)
		  end if
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		myPicture As Picture
	#tag EndProperty


#tag EndWindowCode

